{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/post",
      "dest": "api/index.js"
    },
    {
      "src": "/api/comment",
      "dest": "api/index.js"
    },
    {
      "src": "/api/reply",
      "dest": "api/index.js"
    },
    {
      "src": "/api/user",
      "dest": "api/index.js"
    }
  ]
}
